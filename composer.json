{
  "name": "publishpress/psr-container",
  "version": "2.0.1.1",
  "type": "library",
  "description": "Common Container Interface (PHP FIG PSR-11), prefixed for PublishPress plugins",
  "keywords": [
    "psr",
    "psr-11",
    "container",
    "container-interop",
    "container-interface",
    "publishpress"
  ],
  "homepage": "https://github.com/php-fig/container",
  "license": "MIT",
  "authors": [
    {
      "name": "PHP-FIG",
      "homepage": "https://www.php-fig.org/"
    },
    {
      "name": "PublishPress",
      "email": "help@publishpress.com"
    }
  ],
  "autoload": {
    "files": [
      "src-prefixed/autoload.php"
    ]
  },
  "require": {
    "php": ">=7.2.5"
  },
  "require-dev": {
    "psr/container": "2.0.1",
    "brianhenryie/strauss": "^0.14.0"
  },
  "extra": {
    "strauss": {
      "target_directory": "src-prefixed",
      "namespace_prefix": "PublishPress\\",
      "classmap_prefix": "PublishPress_",
      "constant_prefix": "PUBLISHPRESS_",
      "include_author": "true",
      "classmap_output": "true",
      "packages": [
        "psr/container"
      ],
      "override_autoload": {
      },
      "exclude_from_copy": {
        "packages": [
        ],
        "namespaces": [
        ],
        "file_patterns": [
        ]
      },
      "exclude_from_prefix": {
        "packages": [
        ],
        "namespaces": [
        ],
        "file_patterns": [
        ]
      },
      "namespace_replacement_patterns": {
      },
      "delete_vendor_packages": true,
      "delete_vendor_files": true
    }
  },
  "scripts": {
    "strauss": [
      "vendor/bin/strauss"
    ],
    "post-install-cmd": [
      "@strauss"
    ],
    "post-update-cmd": [
      "@strauss"
    ]
  }
}
